{
  "name": "jest-styled-components-stylelint",
  "version": "0.4.1",
  "description": "A helper for running stylelint on your styled-components styles at runtime.",
  "keywords": [
    "jest",
    "styled-components",
    "stylelint"
  ],
  "author": "Brian Beck <exogen@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/exogen/jest-styled-components-stylelint"
  },
  "files": [
    "src/index.js",
    "src/findComponent.js",
    "src/toPassStylelint.js"
  ],
  "main": "src/index.js",
  "scripts": {
    "format": "prettier --write \"**/*.{js,md}\"",
    "precommit": "lint-staged && npm test",
    "test": "jest"
  },
  "lint-staged": {
    "**/*.{js,md}": [
      "prettier --write",
      "jest --bail --findRelatedTests",
      "git add"
    ]
  },
  "peerDependencies": {
    "styled-components": "^3.1.0",
    "stylelint": "^9.1.1"
  },
  "dependencies": {
    "chalk": "^2.3.2",
    "resolve-from": "^4.0.0"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.5.1",
    "babel-preset-react": "^6.24.1",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "jest-styled-components": "^5.0.0",
    "lint-staged": "^7.0.0",
    "prettier": "^1.11.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "styled-components": "^3.1.6",
    "stylelint": "^9.1.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1"
  }
}
